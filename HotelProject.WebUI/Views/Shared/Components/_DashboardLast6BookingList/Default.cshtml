
@model List<HotelProject.WebUI.Dtos.BookingDto.Result6bookDto>

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="active-member">
                    <div class="table-responsive">
                        <table class="table table-xs mb-0">
                            <thead>
                                <tr>
                                    <th>Müşteri</th>
                                    <th>Şehir</th>
                                    <th>Ülke</th>
                                    <th>Durum</th>
                                   
                                </tr>
                            </thead>
                            <tbody>
                                @foreach(var item in Model) {

                                <tr>
                                    <td>@item.Name</td>
                                    <td>@item.City </td>
                                    <td>
                                        <span>@item.Country</span>
                                    </td>
                                    <td>
                                        <div>
                                            <div class="progress" style="height: 6px">
                                                <div class="progress-bar bg-success" style="width: 100%"></div>

                                            </div>

                                        </div>
                                    <td> @item.Status </td>
                                    </td>


                                </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>