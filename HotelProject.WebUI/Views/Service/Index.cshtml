@model List<ResualtServiceDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Hizmet Başlığı</th>
       
        <th>Sil</th>
        <th>Güncelle</th>
  

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ServiceID</td>
            <td>@item.Title</td>
         
            <td><a href="/Service/DeleteStaff/@item.ServiceID" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
            <td><a href="/Service/UpdateStaff/@item.ServiceID" class="btn mb-1 btn-rounded btn-outline-secondary">Güncelle</a></td>
         
        </tr>
    }
</table>
<a href="/Service/AddService/" class="btn mb-1 btn-rounded btn-outline-info">Yeni Personel Girişi</a>



